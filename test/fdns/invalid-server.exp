set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "pkill fdns\r"
sleep 2

send -- "fdns --server=blablabla\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"cannot connect to server blablabla"
}

after 100
puts "\nall done\n"
